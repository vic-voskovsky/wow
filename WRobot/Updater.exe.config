<?xml version="1.0"?>
<configuration>
  <startup>
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
    </startup>
    <system.net>
      <settings>
        <servicePointManager expect100Continue="false" />
      </settings>
